<mvc:View
    controllerName="ui5.sapui5demo.controller.App"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc">
    <Page title="{i18n>homePageTitle}">
        <headerContent>
            <Button
                icon="sap-icon://hello-word"
                press=".onOpenDialog">
            </Button>
        </headerContent>
        <content>
            <mvc:XMLView viewName="ui5.sapui5demo.view.HelloPanel"></mvc:XMLView>
            <mvc:XMLView viewName="ui5.sapui5demo.view.InvoiceList"></mvc:XMLView>
        </content>
    </Page>

</mvc:View>